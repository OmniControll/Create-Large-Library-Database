import requests
from bs4 import BeautifulSoup

url = "https://www.tradingview.com/symbols/AMZN/"

response = requests.get(url)

if response.status_code == 200:
    soup = BeautifulSoup(response.text, 'html.parser')
    
    # Find the financial data container
    data_container = soup.find('div', class_='tv-symbol-profile__data')

    if data_container:
        # Extract and print financial data
        for item in data_container.find_all('div', class_='tv-symbol-profile__data-row'):
            label = item.find('span', class_='tv-symbol-profile__data-label').text.strip()
            value = item.find('span', class_='tv-symbol-profile__data-value').text.strip()
            print(f"{label}: {value}")
    else:
        print("No financial data found.")
else:
    print(f"Failed to fetch data. HTTP status code: {response.status_code}")
